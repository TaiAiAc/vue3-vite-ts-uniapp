{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/tab-bar/component/component",
      "style": {
        "navigationBarTitleText": "内置组件"
      }
    },
    {
      "path": "pages/tab-bar/API/API",
      "style": {
        "navigationBarTitleText": "接口"
      }
    },
    {
      "path": "pages/tab-bar/extUI/extUI",
      "style": {
        "navigationBarTitleText": "扩展组件"
      }
    }
  ],
  "subPackages": [
    {
      "root": "pages/login",
      "name": "login",
      "pages": [
        {
          "path": "pages/main/index",
          "style": {
            "navigationBarTitleText": "登录"
          }
        }
      ]
    }
  ],
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#3cc51f",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/tab-bar/component/component",
        "iconPath": "static/component.png",
        "selectedIconPath": "static/componentHL.png",
        "text": "内置组件"
      },
      {
        "pagePath": "pages/tab-bar/API/API",
        "iconPath": "static/api.png",
        "selectedIconPath": "static/apiHL.png",
        "text": "接口"
      },
      {
        "pagePath": "pages/tab-bar/extUI/extUI",
        "iconPath": "static/extui.png",
        "selectedIconPath": "static/extuiHL.png",
        "text": "扩展组件"
      }
    ]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}
